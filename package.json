{
  "name": "visa-game",
  "version": "1.0.0",
  "description": "LEO VISA Game",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.base.config.js --open --hot --history-api-fallback --env.PLATFORM=local --port 3000 --host 0.0.0.0",
    "build-test": "webpack -p --config webpack.prod.config.js --env.PLATFORM=test --progress",
    "build-release-test": "webpack -p --config webpack.prod.config.js --env.PLATFORM=production --env.MODE=test --progress",
    "build-release-live": "webpack -p --config webpack.prod.config.js --env.PLATFORM=production --env.MODE=live --progress",
    "publish-test-sco": "npm run build-test && node ScormPackager.js --filename=testbuild",
    "publish-release-test-sco": "npm run build-release-test && node ScormPackager.js --filename=cornerstonetest",
    "publish-release-live-sco": "npm run build-release-live && node ScormPackager.js --filename=cornerstonelive",
    "publish-local-sco": "rm -rf dist build && npm run build-test && copyfiles -f src/ScormTester* build && mkdir dist && folder-zip-sync build dist/visa_game_test_sco.zip",
    "sco-tester": "http-server ./build ",
    "lint-js": "eslint --ext .js --ext .jsx --ext .json .",
    "lint-sass": "sass-lint -v -q",
    "lint": "npm run lint-js && npm run lint-sass",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch"
  },
